{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Processing": {
    "MaxConcurrency": 4,
    "CpuThreshold": 85.0,
    "MemoryThreshold": 85.0
  },
  "CleanupService": {
    "CleanupIntervalMinutes": 2,
    "FileMaxAgeMinutes": 4
  }
}
